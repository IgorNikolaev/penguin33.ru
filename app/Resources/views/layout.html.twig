{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets
        'assets/styles/vendor/bootstrap.css'
        'assets/styles/vendor/bootstrap-theme.css'
        filter='?cssembed'
        filter='cssrewrite'
        filter='?yui_css'
        output='assets/compiled/app.css'
    %}
        <link href="{{ asset_url }}" rel="stylesheet">
    {% endstylesheets %}
{% endblock %}

{% block body %}

    {% block content %}{% endblock %}

{% endblock %}

{% block javascripts %}
    {% javascripts
        'assets/scripts/vendor/jquery.js'
        'assets/scripts/vendor/bootstrap.js'
        filter='?closure'
        output='assets/compiled/app.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
